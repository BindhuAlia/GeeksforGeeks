class Solution{
public:	
	int count(int arr[], int n, int x) {
	    /*int m=*max_element(arr,arr+n);
	    vector<int>ans(m+1,0);
	    for(int i=0;i<n;i++)
	    {
	        ans[arr[i]]+=1;
	    }
	    return ans[x];*/
	    int c=0;
	    for(int i=0;i<n;i++)
	    {
	        if(arr[i]==x)c+=1;
	    }
	    return c;
	}
};
