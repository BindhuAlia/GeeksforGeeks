 void rearrange(long long *arr, int n) 
    { 
    	
    	vector<int>v;
    	int i=0,last=n-1;
    	while(i<=last)
    	{
    	    v.push_back(arr[last]);
    	    v.push_back(arr[i]);
    	    i++;
    	    last--;
    	    
    	}
    	for(int i=0;i<n;i++)
    	{
    	    arr[i]=v[i];
    	}
    	 
    }
};
